%fieldset
  %legend Briefing
  .row
    .span5
      = form.labeled_input :title, :text_field, :class => 'input-block-level input-label'
      = form.labeled_input :description, :text_area
      = form.labeled_input :manager
    .span5
      / = form.labeled_input :start_date, :date_field
      / = form.labeled_input :end_date, :date_field
      = form.labeled_input :status_id, :domain_field, :domain => Projects::Status
      = render 'form_colaborators', :form => form
